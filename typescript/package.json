{
  "name": "@dployr-io/dployr-sdk",
  "version": "1.3.0",
  "type": "module",
  "description": "TypeScript SDK generated with Microsoft Kiota",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "generate": "kiota generate -l typescript -d ./openapi.yaml -o ./client --class-name ApiClient",
    "build": "tsc",
    "start": "node dist/examples/usage.js",
    "clean": "rm -rf dist client"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@microsoft/kiota-bundle": "^1.0.0-preview.99"
  }
}
